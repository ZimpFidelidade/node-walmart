{
  "name": "walmart-b2b",
  "version": "1.0.0",
  "description": "A library for Walmart API",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint": "eslint index.js",
    "david": "david",
    "mocha": "mocha --reporter spec",
    "cover": "istanbul cover _mocha -- -R spec test/* && istanbul check-coverage --statement 100 --branch 100 --line 100 --function 100",
    "test": "npm run eslint && npm run david && npm run mocha && npm run cover",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "preversion": "git pull && rm -rf ./node_modules/ && npm install && npm test",
    "postversion": "git push && npm publish --access=public && git push --tags"
  },
  "pre-commit": [
    "precommit-msg",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZimpFidelidade/node-walmartr.git"
  },
  "keywords": [
    "password",
    "random",
    "string"
  ],
  "author": "Luan Muniz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZimpFidelidade/node-walmart/issues"
  },
  "homepage": "https://github.com/iZmpFidelidade/node-walmartr#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "david": "^9.0.0",
    "eslint": "^3.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "got": "^6.5.0",
    "joi": "^9.2.0"
  }
}
