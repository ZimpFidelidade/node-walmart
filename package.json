{
  "name": "walmart-b2b",
  "version": "1.0.0",
  "description": "A library for Walmart API",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint": "eslint --ignore-path .npmignore **/**/*.js --quiet",
    "david": "david",
    "lab": "lab --verbose --colors --assert code -e development",
    "test-cov": "npm run lab -- -t 93 -r console -o stdout -r html -o coverage/coverage.html -r lcov -o coverage/lcov.info",
    "test": "npm run eslint && npm run david && npm run test-cov",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "preversion": "git pull && rm -rf ./node_modules/ && npm install && npm test",
    "postversion": "git push && npm publish --access=public && git push --tags"
  },
  "pre-commit": [
    "precommit-msg",
    "test"
  ],
  "david": {
    "ignore": [
      "jszip"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZimpFidelidade/node-walmartr.git"
  },
  "keywords": [
    "walmart",
    "api",
    "lib",
    "b2b"
  ],
  "author": "Luan Muniz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZimpFidelidade/node-walmart/issues"
  },
  "homepage": "https://github.com/iZmpFidelidade/node-walmartr#readme",
  "devDependencies": {
    "code": "^4.0.0",
    "coveralls": "^2.11.15",
    "david": "^9.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.7.0",
    "lab": "^11.2.2",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "@luanmuniz/password-generator": "^1.0.3",
    "got": "^6.6.3",
    "joi": "^10.1.0",
    "jszip": "^2.6.1"
  }
}
